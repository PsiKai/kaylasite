<section id="frontpage-carousel">
	<div id="carousel" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
     	<% randomArt.forEach((artWork, i) => { %>
      <div class="carousel-item container-fluid <%= i === 0 ? 'active' : ''%>">
        	<a href="<%= artWork.category %>#<%= artWork.alt %>">
            <img loading="lazy" class="img-fluid" src="<%= artWork.thumbnail %>" alt="<%= artWork.alt %>" class="d-block w-100"/>
					</a>
      	</div>
    	<% }) %>
		</div>
    <a class="carousel-control-prev" href="#carousel" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    	<span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carousel" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
    	<span class="sr-only">Next</span>
  	</a>
	</div>
</section>
